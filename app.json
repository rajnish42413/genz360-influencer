{
  "name": "GenZ360",
  "displayName": "Genz360",
  "expo": {
    "name": "Genz360",
    "slug": "GenZ360_influencer_app",
    "privacy": "unlisted",
    "sdkVersion": "34.0.0",
    "version": "9.1.2",
    "icon": "./images/logo.png",
    "splash": {
      "image": "./images/gi.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "entryPoint": "node_modules/expo/AppEntry.js",
    "ios": {
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to update user profile photo and to upload creativity."
      },
      "bundleIdentifier": "com.genz360.influencer",
      "supportsTablet": true
    },
    "android": {
      "package": "com.genz360_inf",
      "versionCode": 12,
      "permissions": [
        "CAMERA",
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "SYSTEM_ALERT_WINDOW"
      ]
    },
    "androidStatusBar": {
      "backgroundColor": "#111B74"
    },
    "platforms": [
      "android",
      "ios"
    ],
    "description": "",
    "notification": {
      "icon": "./images/logo.png",
      "androidMode": "default"
    }
  }
}